{
  "env": {
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": "> 0.5%, last 2 versions, Firefox ESR, not dead, not ie 11, not ie_mob 11",
            "bugfixes": true,
            "corejs": {
              "version": "3.27",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "node": "16"
            },
            "bugfixes": true,
            "corejs": {
              "version": "3.27",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    },
    "umd": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": "defaults, node >= 14, not ie 11, not ie_mob 11",
            "bugfixes": true,
            "corejs": {
              "version": "3.27",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    },
    "iife": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": "defaults, not ie 11, not ie_mob 11",
            "bugfixes": true,
            "corejs": {
              "version": "3.27",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    }
  }
}
